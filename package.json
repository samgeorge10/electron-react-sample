{
  "name": "electron-react-sample",
  "version": "1.2.0",
  "description": "This is a sample application developed with electron, react and webpack",
  "main": "./src/index.js",
  "scripts": {
    "dev": "webpack --config config/webpack/dev.config.js",
    "build": "webpack --config config/webpack/prod.config.js",
    "lint": "eslint --ext .jsx,.js src config",
    "lint:fix": "eslint --fix --ext .jsx,.js src config",
    "start": "webpack-dev-server --hot --disabled-host-check --config config/webpack/dev.config.js",
    "start:prod": "npm run build && electron --noDevServer ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samgeorge10/electron-react-sample.git"
  },
  "author": "Sam George",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samgeorge10/electron-react-sample/issues"
  },
  "homepage": "https://github.com/samgeorge10/electron-react-sample#readme",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "clean-webpack-plugin": "^3.0.0",
    "electron": "^9.0.5",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "html-webpack-plugin": "^4.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "keywords": [
    "electron-application",
    "electron-webpack",
    "electron-webpack-application",
    "electron-react-application",
    "electron-react-webpack"
  ]
}
